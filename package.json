{
	"name": "MoradaApp.Api",
	"main": "dist/src/main.js",
	"author": "MoradaAPP",
	"license": "MIT",
	"engines": {
		"node": "18"
	},
	"scripts": {
		"build": "nest build",
		"build:watch": "nest build -w",
		"dev": "dotenv -e .env -- nest start --watch",
		"start": "dotenv -e .env.prod node .",
		"emulator": "firebase emulators:start --only functions:SiginAPI,database,auth,firestore",
		"emulator:debug": "firebase emulators:start --inspect-functions --only functions,database,auth,firestore",
		"shell": "firebase functions:shell",
		"firebase:start": "shell",
		"firebase:deploy": "firebase deploy --only functions",
		"firebase:logs": "firebase functions:log",
		"lint": "eslint \"{src,tests}/**/*.ts\"",
		"lint:fix": "eslint --fix",
		"format": "prettier --write",
		"test": "dotenv -e .env.test jest",
		"test:cov": "dotenv -e .env.test jest --coverage",
		"test:e2e": "dotenv -e .env.prod jest --config ./jest.e2e.config.ts",
		"test:e2e:cov": "dotenv -e .env.prod jest --config ./jest.e2e.config.ts --coverage",
		"set-hooks": "husky install",
		"pre-commit": "lint-staged --allow-empty"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"(src|tests)/**/*.(t|j)s": [
			"pnpm format",
			"pnpm lint:fix"
		]
	},
	"prisma": {
		"schema": "./src/infra/storages/db/prisma/schema.prisma"
	},
	"dependencies": {
		"@google-cloud/functions-framework": "^3.3.0",
		"@nestjs/common": "^10.2.10",
		"@nestjs/core": "^10.2.10",
		"@nestjs/event-emitter": "^2.0.3",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.2.10",
		"@nestjs/swagger": "^7.1.17",
		"@nestjs/throttler": "^5.1.1",
		"@prisma/client": "^5.7.0",
		"bcrypt": "^5.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.3.1",
		"dotenv-cli": "^7.3.0",
		"echoguard": "0.0.7-alpha",
		"express": "^4.18.2",
		"firebase-admin": "^11.11.1",
		"firebase-functions": "^4.5.0",
		"husky": "^8.0.3",
		"ioredis": "^5.3.2",
		"nodemailer": "^6.9.7",
		"passport-jwt": "^4.0.1",
		"prisma": "^5.7.0",
		"reflect-metadata": "^0.1.14",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.8.1",
		"@nestjs/cli": "^10.2.1",
		"@nestjs/testing": "^10.2.10",
		"@types/bcrypt": "^5.0.2",
		"@types/cookie-parser": "^1.4.6",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.4",
		"@types/nodemailer": "^6.4.14",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"commitizen": "^4.3.0",
		"commitlint": "^17.8.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.55.0",
		"eslint-config-standard-with-typescript": "^39.1.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.4.0",
		"eslint-plugin-promise": "^6.1.1",
		"firebase-functions-test": "^3.1.0",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"typescript": "^5.3.3"
	}
}
